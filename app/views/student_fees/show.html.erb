<div class="row">
  <div class="col-sm-12"><h1>Student Fee Detail</h1></div>
</div>
<br />

<div class="row">
  <div class="col-sm-4"><strong>Name:</strong></div>
  <div class="col-sm-8"><%= @student_fee.student.name %></div>
</div>
<br />

<div class="row">
  <div class="col-sm-4"><strong>Father:</strong></div>
  <div class="col-sm-8"><%= @student_fee.student.father_name %></div>
</div>
<br />

<div class="row">
  <div class="col-sm-4"><strong>School Branch:</strong></div>
  <div class="col-sm-8"><%= @student_fee.student.school_branch.name %></div>
</div>
<br />
<div class="row">
  <div class="col-sm-4"><strong>Paid date:</strong></div>
  <div class="col-sm-8"><%= @student_fee.paid_date %></div>
</div>
<br />
<div class="row">
  <div class="col-sm-4"><strong>Paid fee:</strong></div>
  <div class="col-sm-8"><%= @student_fee.paid_fee %></div>
</div>
<br />
<div class="row">
  <div class="col-sm-4"><strong>Balance:</strong></div>
  <div class="col-sm-8"><%= @student_fee.balance %></div>
</div>
<br />
<div class="row">
  <div class="col-sm-4"><strong>Fee type:</strong></div>
  <div class="col-sm-8"><%= @student_fee.fee_type %></div>
</div>
<br />
<div class="row">
  <div class="col-sm-4"><%= link_to 'Student Fee', student_fees_path, class: "btn btn-info" %></div>
  <div class="col-sm-2"><%= link_to 'Edit', edit_student_fee_path(@student_fee), class: "btn btn-primary" %></div>
</div>
