<div class="row">
  <div class="col-sm-12"><h1>Class Detail</h1></div>
</div>
<br />
<div class="row">
  <div class="col-sm-4"><strong>ID</strong></div>
  <div class="col-sm-8"><%= @level.id %></div>
</div>
<br />
<div class="row">
  <div class="col-sm-4"><strong>Name</strong></div>
  <div class="col-sm-8"><%= @level.name %></div>
</div>

<br />
<div class="row">
  <div class="col-sm-4"><strong>School Branch</strong></div>
  <div class="col-sm-8"><%= @level.school_branch.name %></div>
</div>
<br />
<div class="row">
  <div class="col-sm-4"><strong>Comment</strong></div>
  <div class="col-sm-8"><%= @level.comment %></div>
</div>
<br />
<div class="row">
  <div class="col-sm-4"><%= link_to 'Classes', levels_path, class: "btn btn-info" %></div>
  <div class="col-sm-2"><%= link_to 'Edit', edit_level_path(@level), class: "btn btn-primary" %></div>
</div>
